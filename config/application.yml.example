# Use `rake secret` to generate one key, Only needed in production environment
SECRET_KEY_BASE: ''

# Your published app's HOST, Used by emailer/payment in production environment
DOMAIN: ''
# Your published app's PROTOCOL, Used by emailer/payment in production environment
PROTOCOL: 'https'

# Browser base title
APPNAME: 'ClackyAI APP'

# OAuth provider credentials generated by authentication generator
# Set enabled: 'true' to activate each OAuth provider (use strings to avoid Figaro warnings)

CLACKY_AUTH_CLIENT_ID: ''
CLACKY_AUTH_CLIENT_SECRET: ''
CLACKY_AUTH_DOMAIN: ''

# Google OAuth
GOOGLE_OAUTH_ENABLED: 'false'
GOOGLE_CLIENT_ID: ''
GOOGLE_CLIENT_SECRET: ''

# Facebook OAuth
FACEBOOK_OAUTH_ENABLED: 'false'
FACEBOOK_APP_ID: ''
FACEBOOK_APP_SECRET: ''

# Twitter OAuth
TWITTER_OAUTH_ENABLED: 'false'
TWITTER_API_KEY: ''
TWITTER_API_SECRET: ''

# GitHub OAuth
GITHUB_OAUTH_ENABLED: 'false'
GITHUB_CLIENT_ID: ''
GITHUB_CLIENT_SECRET: ''
# OAuth provider credentials generated end
